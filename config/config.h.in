#ifndef INCLUDE_MVOX_CONFIG_H
#define INCLUDE_MVOX_CONFIG_H

// version as integer of the form: (major*100 + minor)*100 + patch.
#cmakedefine MVOX_VERSION @MVox_VERSION@

// version string of the form "3.2" or "3.2.1".
#cmakedefine MVOX_VERSION_STRING "@MVox_VERSION_STRING@"

#define MVOX_VERSION_MAJOR "@MVox_VERSION_MAJOR@"
#define MVOX_VERSION_MINOR "@MVox_VERSION_MINOR@"
#define MVOX_VERSION_PATCH "@MVox_VERSION_PATCH@"

#cmakedefine MVOX_DEBUG

#endif // INCLUDE_MVOX_CONFIG_H
